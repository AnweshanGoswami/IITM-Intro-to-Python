    "def check_leap_year(year):\n",
    "    if year%100 ==0 and year%400 == 0:\n",
    "        return True\n",
    "    elif year%100 != 0 and year%4 == 0:\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "\n",
    "print(check_leap_year(2012))\n",
    "print(check_leap_year(2000))\n",
    "print(check_leap_year(2010))"
   ]
  }
