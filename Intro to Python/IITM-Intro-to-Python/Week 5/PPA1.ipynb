    "def factorial(n):\n",
    "    if n<0:\n",
    "        return -1\n",
    "    elif n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n*factorial(n-1)\n",
    "\n",
    "print(factorial(-5))\n",
    "print(factorial(0))\n",
    "print(factorial(5))"
   ]
  }
